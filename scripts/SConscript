#
import sys
SConscript("FileFilter.SConscript")
Import("env")
Import("FilterFilesInto")

print >> sys.stderr, "pkgsurl=%s" % env['pkgsurl']
print >> sys.stderr, "lsst_home=%s" % env['lsst_home']

FilterFilesInto(env, "../etc", Split("newinstall.sh loadLSST.sh loadLSST.csh"))
FilterFilesInto(env, "../bin", Split("lsstpkg mksandbox switcheups tbl2man"))

